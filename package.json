{
  "name": "adonis-fullstack-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node ace test",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "pack":     "cross-env NODE_ENV=development webpack --progress --config=webpack.config.dev.js --watch",
    "packprod": "cross-env NODE_ENV=production webpack --progress --config=webpack.config.prod.js",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.1.0",
    "@adonisjs/bodyparser": "^2.0.9",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.13",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.29",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/websocket": "^1.0.12",
    "@adonisjs/websocket-client": "^1.0.9",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "axios": "^0.19.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "bootstrap": "^4.4.1",
    "cross-env": "^7.0.2",
    "imagemin": "^7.0.1",
    "jquery": "^3.4.1",
    "laravel-mix": "^5.0.4",
    "lodash": "^4.17.15",
    "mysql": "^2.18.1",
    "pem": "^1.14.4",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-habitat": "^1.0.1",
    "sip.js": "^0.15.10",
    "webrtc-adapter": "^7.5.1",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.9.4",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2"
  },
  "autoload": {
    "App": "./app"
  }
}
